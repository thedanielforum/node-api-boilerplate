{
  "name": "api-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src --out-dir build",
    "prod": "pm2 start build --name api",
