# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*.js]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true

[*.json]
end_of_line = lf
insert_final_newline = false
indent_style = space
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true

[*.gradle]
indent_size = 2
